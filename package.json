{
  "name": "fastify-todo-api",
  "version": "1.0.0",
  "description": "REST API for Todo App with Fastify, MySQL and TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "ts-node src/database/migrate.ts",
    "seed": "ts-node src/database/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "mysql2": "^3.6.5",
    "pino-pretty": "^13.1.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.5.0",
    "@types/supertest": "^2.0.15",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
